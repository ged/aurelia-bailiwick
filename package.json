  "devDependencies": {
    "babel": "npm:babel-core@^5.8.21",
    "babel-eslint": "^4.1.3",
    "conventional-changelog": "^0.5.1",
    "core-js": "npm:core-js@^1.0.0",
    "del": "^2.0.2",
    "eslint": "^1.7.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.3.0",
    "gulp-bump": "^1.0.0",
    "gulp-eslint": "^1.0.0",
    "gulp-shell": "^0.5.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-yuidoc": "^0.1.2",
    "jspm": "^0.16.13",
    "karma": "^0.13.14",
    "object.assign": "^4.0.3",
