{
  "name": "aurelia-bailiwick",
  "version": "0.0.1",
  "description": "Aurelia adapters for the Bailiwick domain model toolkit",
  "author": "Michael Granger",
  "license": "BSD-3-Clause",
  "main": "index",
  "registry": "jspm",
  "homepage": "https://github.com/ged/aurelia-bailiwick#readme",
