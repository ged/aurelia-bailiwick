{
  "name": "aurelia-bailiwick",
  "version": "0.0.1",
  "description": "Aurelia adapters for the Bailiwick domain model toolkit",
  "author": "Michael Granger",
  "license": "BSD-3-Clause",
  "main": "dist/system/index",
  "homepage": "https://github.com/ged/aurelia-bailiwick#readme",
  "scripts": {
    "postinstall": "jspm install",
    "test": "karma start --single-run",
    "ci": "karma start --no-single-run",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ged/aurelia-bailiwick.git"
  },
  "keywords": [
    "bailiwick",
    "aurelia",
    "observer",
    "custom-tag"
  ],
  "jspm": {
    "main": "index",
    "format": "register",
    "directories": {
      "lib": "dist/system"
    },
    "dependencies": {
      "aurelia-binding": "npm:aurelia-binding@^0.0.0",
    },
    "peerDependencies": {
      "bailiwick": "github:ged/bailiwick@master"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.21",
      "babel-runtime": "npm:babel-runtime@^5.8.20",
      "core-js": "npm:core-js@^1.0.0"
    }
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "conventional-changelog": "^0.5.1",
    "del": "^2.0.2",
    "eslint": "^1.7.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.3.0",
    "gulp-bump": "^1.0.0",
    "gulp-eslint": "^1.0.0",
    "gulp-shell": "^0.5.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-yuidoc": "^0.1.2",
    "jspm": "^0.16.13",
    "karma": "^0.13.14",
    "object.assign": "^4.0.3",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.4",
    "vinyl-paths": "^2.0.0",
    "yargs": "^3.29.0"
  },
  "dependencies": {}
}
